# AWS OIDC Configuration (Optional)
# AWS_OIDC_ISSUER_URL=https://your-aws-oidc-issuer-url
# AWS_OIDC_CLIENT_ID=your-aws-oidc-client-id
# AWS_OIDC_CLIENT_SECRET=your-aws-oidc-client-secret

# Development Settings
DEBUG=True
